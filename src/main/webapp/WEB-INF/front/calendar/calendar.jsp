<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<table id="calendar">
	<thead>
		<tr>
			<th colspan="7">
				<button id="today">오늘</button>
				<button id="prev">이전</button> <select id="year"></select> <select
				id="month"></select>
				<button id="next">다음</button>
			</th>
		</tr>
		<tr>
			<td class="sun">일</td>
			<td>월</td>
			<td>화</td>
			<td>수</td>
			<td>목</td>
			<td>금</td>
			<td>토</td>
		</tr>
	</thead>
	<tbody>
		<!-- 달력 생성 영역 -->
	</tbody>
</table>
